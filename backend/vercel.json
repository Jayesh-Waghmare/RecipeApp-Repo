{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      }
    ],
    "env": {
      "SPOONACULAR_API_KEY": "@spoonacular_api_key",
      "FRONTEND_URL": "@frontend_url"
    }
  }